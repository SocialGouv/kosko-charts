{
  "dependencies": {
    "@kosko/env": "^3.0.0",
    "@kubernetes-models/sealed-secrets": "^3.0.0",
    "@socialgouv/kosko-charts": "9.19.2",
    "@types/node": "^16.11.14",
    "kosko": "^2.0.0",
    "kubernetes-models": "^2.0.2",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-modules-commonjs": "^7.16.5",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.3",
    "dotenv": "^10.0.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.5",
    "tempy": "^1.0.1",
    "typescript": "^4.5.4"
