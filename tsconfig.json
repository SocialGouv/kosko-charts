{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "importHelpers": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "paths": { "@socialgouv/kosko-charts/*": ["./src/*"] },
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "ESNext",
    "types": ["dotenv", "jest", "node"]
  },
  "exclude": ["components", "coverage", "environments", "types"]
}
